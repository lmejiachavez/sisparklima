
<%= form_tag({:controller => "welcome"}, :method => "get") do %>
	<p>
		<%= text_field_tag :search, params[:search]%>
		<%= submit_tag "Buscar", :name=> nil %>
	</p>
<% end %>



<table class="table table-hover">
 <thead>
